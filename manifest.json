{
   "author": "Szymon Pilkowski",
   "background": {
      "page": "init.html"
   },
   "browser_action": {
      "default_icon": {
         "19": "action.png",
         "38": "action.png"
      },
      "default_popup": "background.html",
      "default_title": "Faster Bookmark Button"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "chromeos": "Ctrl+B",
            "default": "Ctrl+B",
            "linux": "Ctrl+B",
            "mac": "Command+B",
            "windows": "Ctrl+B"
         }
      }
   },
   "description": "Easily add bookmarks to any category. Includes spotlight-like search with mouse/keyboard support. Default key: cmd+b (win:ctrl+b)",
   "homepage_url": "https://github.com/ardcore/chrome-better-bookmark",
   "icons": {
      "128": "bookmark.png",
      "16": "bookmark.png",
      "48": "bookmark.png"
   },
   "manifest_version": 2,
   "name": "Faster Bookmark",
   "permissions": [ "bookmarks", "tabs" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.3.1"
}
